<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white</item>
    </style>

    <style name="CustomBottomNavigationView" parent="Widget.Design.BottomNavigationView">
        <item name="itemRippleColor">@color/transparent</item>
    </style>

    <style name="CustomEditText" parent="Widget.AppCompat.EditText">
        <item name="android:fontFamily">@font/lilita_one</item>
    </style>

    <style name="CustomAlertDialog" parent="Theme.AppCompat.Dialog">
        <item name="android:background">@color/transparent</item> <!-- Reference your drawable -->
        <item name="android:backgroundDimEnabled">false</item>
    </style>

</resources>